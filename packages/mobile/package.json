{
  "name": "WorkTalkV2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "cleanup": "cd android && ./gradlew clean",
    "-------build-dev-------": "-------dev-------",
    "android-dev-debug": "ENVFILE=.env.dev react-native run-android --variant=devDebug",
    "android-dev-release": "cd android && ENVFILE=.env.dev ./gradlew assembleDev",
    "ios-dev-debug-simulator": "react-native run-ios --simulator=\"iPhone 11\" --scheme \"WorkTalkV2-dev\"",
    "-------build-staging-------": "-------staging-------",
    "android-staging-debug": "ENVFILE=.env.staging react-native run-android --variant=stagingDebug",
    "android-staging-release": "cd android && ENVFILE=.env.staging ./gradlew assembleStaging",
    "ios-staging-debug": "react-native run-ios --simulator=\"iPhone 11\"  --scheme \"WorkTalkV2-staging\"",
    "-------build-production-------": "-------production-------",
    "android-production-debug": "ENVFILE=.env.production react-native run-android --variant=productionDebug",
    "android-production-release": "cd android && ENVFILE=.env.production ./gradlew assembleProduction",
    "ios-production-debug": "react-native run-ios --simulator=\"iPhone 11\" --scheme \"WorkTalkV2-production\"",
    "-------code-push-------": "-------code-push-------",
    "cp-android-staging": "appcenter codepush release-react -a hyper.ihcm-gmail.com/work-talk-android-v2-staging -b app.bundle -d Staging",
    "cp-android-production": "appcenter codepush release-react -a hyper.ihcm-gmail.com/work-talk-android-v2-production -b app.bundle -d Staging",
    "cp-ios-staging": "appcenter codepush release-react -a hyper.ihcm-gmail.com/work-talk-ios-v2-staging -p ios/WorkTalkV2/WorkTalkV2-info-staging.plist -d Staging",
    "cp-ios-production": "appcenter codepush release-react -a hyper.ihcm-gmail.com/work-talk-ios-v2-production -p ios/WorkTalkV2/WorkTalkV2-info-production.plist -d Staging"
  },
